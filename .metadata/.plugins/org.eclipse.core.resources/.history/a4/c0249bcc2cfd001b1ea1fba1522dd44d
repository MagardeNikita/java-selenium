package test1;

public class ArrayOfArray {
	
	public static void main(String args[])
	{
		int[][] array= new int[2][2];
		
		array[0][0]= 2;
		array[0][1]= 3;
		array[1][0]= 2;
		array[1][1]= 2;
		
		for (int[] n : array) {
			for (int data : n) {
				System.out.println(data);
			}
			
		}
		
		int[][] array2= {{1,2}, {3,4}, {6,7,8,9}};
		
		
		
	}

}
